<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.bignerdranch.android.DustApp.MainActivity">


    <Button
        android:id="@+id/button"
        android:layout_width="191dp"
        android:layout_height="wrap_content"
        android:onClick="onButton1Clicked"
        android:text="여지은꺼 켜기" />

    <Button
        android:id="@+id/button2"
        android:layout_width="191dp"
        android:layout_height="wrap_content"
        android:onClick="onButton2Clicked"
        android:text="여지은꺼 끄기" />

    <Button
        android:id="@+id/button5"
        android:layout_width="191dp"
        android:layout_height="wrap_content"
        android:onClick="onButton3Clicked"
        android:text="문소정꺼 켜기" />

    <Button
        android:id="@+id/button6"
        android:layout_width="191dp"
        android:layout_height="wrap_content"
        android:onClick="onButton4Clicked"
        android:text="문소정꺼 끄기" />

</LinearLayout>
